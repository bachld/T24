    SUBROUTINE AC.RTN.TYPE
    $INSERT I_COMMON
    $INSERT I_EQUATE
    $INSERT I_ENQUIRY.COMMON
    $INSERT I_F.ACCOUNT
    Y.CATE=R.RECORD<AC.CATEGORY>
    BEGIN CASE
    CASE Y.CATE EQ 1001
        O.DATA='TAI KHOAN THANH TOAN'
    CASE Y.CATE EQ 6001
        O.DATA='TAI KHOAN TIET KIEM'
    CASE 1
        O.DATA='TAI KHOAN KHAC'
    END CASE
    RETURN
END
